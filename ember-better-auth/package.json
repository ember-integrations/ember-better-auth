    "@embroider/vite": "^1.2.0",
    "@glint/core": "^2.0.0-alpha.4",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.3",
    "@glint/template": "^1.6.0-alpha.2",
    "@gossi/config-eslint": "^1.3.0",
    "@gossi/config-prettier": "^1.1.0",
    "@gossi/config-targets": "^1.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.13",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "better-auth": "^1.3.7",
    "ember-qunit": "^9.0.3",
    "ember-source": "^6.6.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.34.0",
    "eslint-plugin-ember": "^12.7.3",
    "eslint-plugin-qunit": "^8.2.5",
    "prettier": "^3.6.2",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "testem": "^3.15.1",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
