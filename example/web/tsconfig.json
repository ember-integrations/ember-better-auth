{
  "extends": "@ember/app-tsconfig",
  "include": [
    "src",
    "tests",
    "types"
  ],
  "compilerOptions": {
    "allowJs": true,
    "paths": {
      "#/*": [
        "./src/*"
      ],
      "*": [
        "./types/*"
      ]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "@glint/ember-tsc/types",
      "vite/client"
    ]
  },
}
