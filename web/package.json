{
  "name": "web-app",
  "private": true,
  "license": "MIT",
  "type": "module",
  "imports": {
    "#/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix\"",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "test": "vite build --mode development && testem ci"
  },
  "dependencies": {
    "ember-better-auth": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.28.3",
    "@ember/app-tsconfig": "^1.0.3",
    "@embroider/core": "^4.1.3",
    "@embroider/macros": "^1.18.1",
    "@embroider/vite": "^1.2.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.4",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.3",
    "@glint/template": "^1.6.0-alpha.2",
    "@gossi/config-eslint": "^1.3.1",
    "@gossi/config-prettier": "^1.1.1",
    "@gossi/config-stylelint": "^1.1.1",
    "@gossi/config-template-lint": "^1.2.0",
    "@hokulea/core": "^0.6.0",
    "@hokulea/ember": "^0.9.0",
    "@hokulea/theme-moana": "^0.2.0",
    "@iconify-json/ph": "^1.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@theemo/ember": "^1.0.1",
    "@theemo/vite": "^1.0.1",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "better-auth": "^1.3.7",
    "decorator-transforms": "^2.3.0",
    "ember-ability": "^1.0.0",
    "ember-command": "^2.0.7",
    "ember-link": "^3.5.0",
    "ember-resources": "^7.0.7",
    "ember-source": "~6.6.0",
    "ember-strict-application-resolver": "^0.1.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.38.0",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-qunit": "^8.2.5",
    "glimmer-scoped-css": "^0.8.1",
    "prettier": "^3.6.2",
    "reactiveweb": "^1.9.0",
    "stylelint": "^16.21.0",
    "testem": "^3.16.0",
    "typescript": "^5.9.2",
    "unplugin-icons": "^22.2.0",
    "valibot": "^1.1.0",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
